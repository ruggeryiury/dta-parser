{
	"name": "@ruggeryiury/dta-parser",
	"version": "1.1.0",
	"private": false,
	"description": "A Rock Band song metadata file parser written in TypeScript",
	"main": "lib/index.js",
	"module": "module/index.js",
	"types": "lib/index.d.ts",
	"author": "Ruggery Iury CorrÃªa <ruggeryiury@gmail.com> (https://ruggeryiury.github.io/my-linktree/)",
	"license": "MIT",
	"keywords": [
		"parser",
		"rock-band",
		"dta"
	],
	"scripts": {
		"build": "yarn clean && yarn build:commonjs && yarn build:esmodule && webpack",
		"build:commonjs": "tsc",
		"build:esmodule": "tsc -m es6 --outDir module",
		"clean": "rimraf dist lib module",
		"format": "prettier --write src/**/*.{js,ts,tsx}",
		"lint": "eslint src",
		"test": "nodemon ./src/tests/test.ts"
	},
	"exports": {
		".": {
			"require": "./lib/index.js",
			"import": "./module/index.js",
			"umd": "./dist/dta-parser.js"
		}
	},
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@babel/core": "^7.22.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/preset-env": "^7.22.4",
		"@babel/preset-typescript": "^7.21.5",
		"@types/babel__core": "^7.20.1",
		"@types/lodash": "^4.14.195",
		"@types/node": "^20.2.5",
		"@types/prettier": "^2.7.3",
		"@types/spotify-api": "^0.0.22",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"babel-loader": "^9.1.2",
		"eslint": "^8.42.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"terser-webpack-plugin": "^5.3.9",
		"ts-loader": "^9.4.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"webpack": "^5.85.1",
		"webpack-bundle-analyzer": "^4.9.0",
		"webpack-cli": "^5.1.3"
	}
}
