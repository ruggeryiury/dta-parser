{
	"name": "@ruggeryiury/dta-parser",
	"version": "1.0.0",
	"private": false,
	"description": "A Rock Band song metadata file parser written in TypeScript",
	"main": "dist/dta-parser.cjs.js",
	"umd:main": "dist/dta-parser.umd.js",
	"module": "dist/dta-parser.mjs",
	"types": "dist/index.d.ts",
	"author": "Ruggery Iury CorrÃªa <ruggeryiury@gmail.com> (https://ruggeryiury.github.io/my-linktree/)",
	"license": "MIT",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"repository": {
		"url": "git@github.com:ruggeryiury/dta-parser.git"
	},
	"keywords": [
		"parser",
		"rock-band",
		"dta"
	],
	"scripts": {
		"build": "yarn run clean && yarn run build:types && npx webpack",
		"build:types": "yarn clean && tsc -p tsconfig.json",
		"clean": "rimraf dist build",
		"format": "prettier --write \"src/**/*.{js,ts,tsx}",
		"lint": "eslint src",
		"test": "nodemon ./src/debug/test.ts"
	},
	"exports": {
		".": {
			"require": "./dist/dta-parser.cjs.js",
			"import": "./dist/dta-parser.mjs"
		}
	},
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@babel/core": "^7.22.1",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.20.7",
		"@babel/preset-env": "^7.22.4",
		"@babel/preset-typescript": "^7.21.5",
		"@types/babel__core": "^7.20.1",
		"@types/lodash": "^4.14.195",
		"@types/node": "^20.2.5",
		"@types/prettier": "^2.7.3",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"babel-loader": "^9.1.2",
		"eslint": "^8.42.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"terser-webpack-plugin": "^5.3.9",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4",
		"webpack": "^5.85.1",
		"webpack-bundle-analyzer": "^4.9.0",
		"webpack-cli": "^5.1.3"
	}
}
