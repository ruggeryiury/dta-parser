{
  "name": "dta-parser",
  "version": "1.0.0",
  "private": false,
  "description": "A Rock Band song metadata file parser written in TypeScript",
  "homepage": "https://github.com/ruggeryiury/dta-parser#readme",
  "repository": "github:ruggeryiury/dta-parser",
  "type": "module",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "lib/index.d.ts",
  "author": "Ruggery Iury CorrÃªa <ruggeryiury@gmail.com>",
  "license": "MIT",
  "keywords": [
    "parser",
    "rock-band",
    "dta",
    "dta-file",
    "javascript",
    "typescript"
  ],
  "scripts": {
    "build": "yarn build:commonjs && yarn build:esmodules",
    "build:package": "yarn build:commonjs && yarn build:esmodules && yarn pack",
    "build:commonjs": "tsc --project ./prod.tsconfig.json",
    "build:esmodules": "tsc --project ./prod.tsconfig.json --module esnext --moduleResolution bundler --outDir module",
    "clear": "rimraf dist lib module",
    "dev": "nodemon ./tests/init.ts",
    "format": "prettier --write .",
    "lint": "eslint src"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./module/index.js",
      "default": "./lib/index.js"
    },
    "./core": {
      "require": "./lib/core.js",
      "import": "./module/core.js",
      "default": "./lib/core.js"
    },
    "./utils": {
      "require": "./lib/utils.js",
      "import": "./module/utils.js",
      "default": "./lib/utils.js"
    }
  },
  "dependencies": {
    "jschardet": "^3.1.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.8",
    "eslint": "^9.2.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-n": "^17.4.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
